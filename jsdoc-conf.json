{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "exclude": ["./node_modules/", "./logs/", "./docs/"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts" : {
        "destination" : "./docs/",
        "recurse" : true,
        "access" : "all",
        "package" : "./package.json",
        "readme" : "./README.md",
        "template" : "./node_modules/ink-docstrap/template"
    }
}